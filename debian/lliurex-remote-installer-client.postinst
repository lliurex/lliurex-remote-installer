#!/bin/sh

set -e

case "$1" in
   configure)
   
	
	n4d-modules enable-plugin /etc/n4d/conf.d/LliurexRemoteInstallerClient || true
	n4d-modules enable-control LliureXRemoteInstallerClient || true
   
	
   ;;

   abort-upgrade|abort-remove|abort-deconfigure)
   ;;

   *)
       exit 1
   ;;
esac

#DEBHELPER#

exit 0

